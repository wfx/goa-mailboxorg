<node>
  <interface name="com.intel.dLeynaServer.Manager">
    <method name="GetServers">
      <arg name="Servers" type="ao" direction="out" />
    </method>
    <signal name="FoundServer">
      <arg type="o" name="Path" />
    </signal>
    <signal name="LostServer">
      <arg type="o" name="Path" />
    </signal>
  </interface>
